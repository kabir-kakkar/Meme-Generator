 </Link>\r\n            </StyledNavItem>\r\n        );\r\n    }\r\n}\r\n\r\nconst NavIcon = styled.div`\r\n`;\r\n\r\nexport default class Sidebar extends React.Component {\r\n    render() {\r\n        return (\r\n            <RouterSideNav></RouterSideNav>\r\n        );\r\n    }\r\n}","C:\\Others\\webdevbootcamp\\meme-generator\\src\\Home.js",[],"C:\\Others\\webdevbootcamp\\meme-generator\\src\\NoMatch.js",[],"C:\\Others\\webdevbootcamp\\meme-generator\\src\\MemeGenerator.js",[],{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","severity":1,"message":"46","line":2,"column":10,"nodeType":"47","messageId":"48","endLine":2,"endColumn":13},{"ruleId":"45","severity":1,"message":"49","line":3,"column":27,"nodeType":"47","messageId":"48","endLine":3,"endColumn":33},{"ruleId":"45","severity":1,"message":"50","line":3,"column":35,"nodeType":"47","messageId":"48","endLine":3,"endColumn":40},"no-native-reassign",["51"],"no-negated-in-lhs",["52"],"no-unused-vars","'Nav' is defined but never used.","Identifier","unusedVar","'Router' is defined but never used.","'Route' is defined but never used.","no-global-assign","no-unsafe-negation"]